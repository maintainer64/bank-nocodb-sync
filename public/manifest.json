{
  "name": "Bank loader to NocoDB",
  "description": "Get csv and API to providers data",
  "version": "1.1",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "cookies",
    "webRequest"
  ],
  "host_permissions": [
    "*://*.tbank.ru/*",
    "*://bank.yandex.ru/*",
    "*://*.nocodb.com/*",
    "https://*/*"
  ]
}